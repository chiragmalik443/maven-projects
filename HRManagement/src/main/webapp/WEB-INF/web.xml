<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>hr_user_login</servlet-name>
  	<display-name>hr_user_login</display-name>
  	<description></description>
  	<servlet-class>hr_user_login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>people_employee</servlet-name>
  	<display-name>people_employee</display-name>
  	<description></description>
  	<servlet-class>people_employee</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>time_management</servlet-name>
  	<display-name>time_management</display-name>
  	<description></description>
  	<servlet-class>time_management</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>hr_user_login</servlet-name>
  	<url-pattern>/hr_user_login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>people_employee</servlet-name>
  	<url-pattern>/people_employee</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>time_management</servlet-name>
  	<url-pattern>/time_management</url-pattern>
  </servlet-mapping>
</web-app>
